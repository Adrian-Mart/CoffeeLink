<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/UI%20Toolkit/StyleSheet.uss?fileID=7433441132597879392&amp;guid=30cfe7c6f7d24df46b63bcf35b3408da&amp;type=3#StyleSheet" />
    <ui:VisualElement name="MainContainer" style="flex-grow: 1; background-color: rgb(255, 255, 255);">
        <ui:VisualElement name="TitleContainer" style="flex-grow: 1;">
            <ui:Label tabindex="-1" text="Coffee Link" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Title" class="title" style="margin-top: 100px;" />
            <ui:VisualElement name="Logo" style="flex-grow: 1; background-image: resource(&apos;Sprites/Logo&apos;); -unity-background-scale-mode: scale-to-fit; flex-shrink: 0; flex-basis: 70%;" />
            <ui:Label tabindex="-1" text="¡Bienvenido!" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Subtitle" class="subtitle" />
