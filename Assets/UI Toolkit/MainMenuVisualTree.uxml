        </ui:VisualElement>
        <ui:VisualElement name="FieldsContainer" style="flex-grow: 1; justify-content: flex-end; align-self: stretch; align-items: center;">
            <ui:TextField picking-mode="Ignore" label="Nombre de usuario" name="UserField" class="textField" style="flex-direction: column; align-self: stretch; justify-content: space-between; align-items: center;" />
            <ui:TextField picking-mode="Ignore" label="Contraseña" name="PasswordField" password="true" class="textField" style="flex-direction: column; align-self: stretch; justify-content: space-around; align-items: center;" />
